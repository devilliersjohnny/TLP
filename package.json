{
  "name": "tlp",
  "version": "0.0.1",
  "scripts": {
    "dev": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender",
    "dev:drafts": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender --buildFuture --buildDrafts",
    "build": "exec-bin node_modules/.bin/hugo/hugo --verbose --gc --cleanDestinationDir --minify",
    "render:dev": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender --renderToDisk --cleanDestinationDir",
    "render:prod": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender --renderToDisk --cleanDestinationDir --environment production --minify",
    "npm-check": "npm-check -u",
    "postinstall": "hugo-installer --version otherDependencies.hugo --extended --destination node_modules/.bin/hugo",
    "hugo-version": "hugo version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devilliersjohnny/TLP.git"
  },
  "author": "Johnny de Villiers",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/devilliersjohnny/TLP/issues"
  },
  "homepage": "https://github.com/devilliersjohnny/TLP#readme",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "autoprefixer": "^10.4.7",
    "bootstrap": "^5.1.3",
    "exec-bin": "^1.0.0",
    "hugo-installer": "^3.1.0",
    "npm-check": "^5.9.2",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0"
  },
  "otherDependencies": {
    "hugo": "0.101.0"
  }
}
